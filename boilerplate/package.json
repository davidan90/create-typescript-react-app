{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "> 8",
    "npm": ">= 5"
  },
  "main": "index.jsx",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open",
    "build": "NODE_ENV=production webpack",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "test": "jest"
  },
  "dependencies": {
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.1",
    "@storybook/addon-info": "3.4.1",
    "@storybook/react": "3.4.1",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/node": "9.6.7",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-links": "^3.3.0",
    "@types/storybook__react": "^3.0.7",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "7.0.5",
    "node-sass": "^4.8.3",
    "prettier": "1.12.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "3.5.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "3.11.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^2.11.2"
  },
  "lint-staged": {
    "*.{tsx}": [
      "tslint",
      "prettier --write",
      "git add"
    ],
    "*.{ts}": [
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/private/jest/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/private/jest/enzymeConfigure.js"
  }
}
